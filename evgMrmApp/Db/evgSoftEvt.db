    field( DESC, "EVG Software Event Code")
    field( DTYP, "Obj Prop uint32")
    field( INP,  "@OBJ=$(OBJ), PROP=EvtCode")
    field( HIHI, "256")
    field( LOLO, "-1")
    field( HHSV, "MAJOR")
    field( LLSV, "MAJOR")
}
